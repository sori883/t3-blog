{
  "extends": "@acme/tsconfig/base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "target": "es5",
    "lib": ["es2015", "es2017"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "noEmit": false
  },
  "baseUrl": "./src",
  "include": ["src/"],
  "exclude": ["node_modules"]
}
